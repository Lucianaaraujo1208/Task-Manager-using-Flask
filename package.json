    "karma-rollup-preprocessor": "^7.0.8",
    "lockfile-lint": "^4.13.1",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^3.29.5",
    "rollup-plugin-istanbul": "^5.0.0",
    "rtlcss": "^4.1.1",
    "sass": "^1.71.0",
    "sass-true": "^8.0.0",
    "serve": "^7.0.0",
    "shelljs": "^0.8.5",
    "stylelint": "^16.2.1",
    "stylelint-config-twbs-bootstrap": "^15.0.0",
    "terser": "^5.27.2",
    "vnu-jar": "23.4.11",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "js/{src,dist}/**/*.{js,map}",
    "js/index.{esm,umd}.js",
    "scss/**/*.scss",
    "!scss/tests/**"
  ],
  "hugo-bin": {
    "buildTags": "extended"
